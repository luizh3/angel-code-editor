{
  "rules": [
    {
      "color": "#0891b2",
      "fontWeight": "",
      "regex": "\\b[A-Za-z]+\\b",
      "italic": false,
      "description": "Any word",
      "words": []
    },
    {
      "color": "#f43f5e",
      "fontWeight": "",
      "regex": "\\b[A-Za-z]+\\b",
      "italic": false,
      "description": "Reserved words",
      "words": [
        "class",
        "return",
        "await",
        "catch",
        "this",
        "throw",
        "new",
        "try"
      ]
    },
    {
      "color": "#0891b2",
      "fontWeight": "",
      "regex": "\\b[A-Za-z0-9_]+(?=\\()",
      "italic": false,
      "description": "Functions",
      "words": []
    },
    {
      "color": "#f59e0b",
      "fontWeight": "",
      "regex": "[\\(\\)\\{\\}\\[\\]]",
      "italic": false,
      "description": "Simbols, {}, (), []",
      "words": []
    },
    {
      "color": "#14b8a6",
      "fontWeight": "",
      "regex": "'.*'",
      "italic": false,
      "description": "Simple quotation marks",
      "words": []
    },
    {
      "color": "#14b8a6",
      "fontWeight": "",
      "regex": "\".*\"",
      "italic": false,
      "description": "Double quotation marks",
      "words": []
    },
    {
      "color": "#fdba74",
      "fontWeight": "",
      "regex": "\\(([^()]*)\\)|\\[([^\\[\\]]*)\\]",
      "italic": false,
      "description": "Params",
      "words": []
    }
  ]
}
